{
    "name": "innmind/rest-server-bundle",
    "type": "library",
    "description": "Symfony integration of innmind/rest-server",
    "keywords": ["rest", "api"],
    "homepage": "http://github.com/Innmind/RestServerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/RestServerBundle/issues"
    },
    "require": {
        "php": "~7.0",
        "innmind/rest-server": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/routing": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "innmind/http": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Rest\\ServerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Rest\\ServerBundle\\": "tests/",
            "Fixtures\\Innmind\\Rest\\ServerBundle\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
